    "csp-webpack-plugin": "^2.0.2",
    "dotenv": "8.0.0",
    "gatsby": "2.13.52",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-google-analytics": "2.1.7",
    "gatsby-plugin-manifest": "2.2.5",
    "gatsby-plugin-netlify": "2.1.3",
    "gatsby-plugin-netlify-cache": "1.2.0",
