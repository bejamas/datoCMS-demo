    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "prettier-standard": "^9.1.1",
    "standard": "^13.1.0"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
